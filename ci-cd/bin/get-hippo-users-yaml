#!/bin/bash

# curent folder
DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )

function main {
    local users_file=$1
    local developers_users_file=$2

    cat "${DIR}/../users.base.yaml"
    while read -r line; do
        ${DIR}/get-hippo-test-user-yaml "${line}"
    done < "$users_file"

    while read -r line; do
        ${DIR}/get-hippo-user-yaml "${line}"
    done < "$developers_users_file"

}

main "$@"
