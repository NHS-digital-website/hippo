#!/bin/bash

## Return OnDemand environment name

case $1 in
    dev)
        echo "dev";
        ;;
    prd)
        echo "prod";
        ;;
    trg)
        echo "training";
        ;;
    training)
        echo "training";
        ;;
    trn)
        echo "training";
        ;;
    tst)
        echo "test";
        ;;
    uat)
        echo "acct";
        ;;
    *)
        echo "unknown"
        ;;
esac;
