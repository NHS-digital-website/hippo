<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="microsoft-graph-api-spring-beans.xml" />

    <bean id="uk.nhs.digital.intranet.components.HomePageComponent" class="uk.nhs.digital.intranet.components.HomePageComponent" scope="prototype">
        <constructor-arg name="applicationId" ref="applicationId" />
        <constructor-arg name="redirectUri" ref="redirectUri" />
    </bean>

</beans>
