                <entry key="releasestatuses" value="/content/documents/administration/website/release-statuses"/>
                <entry key="parametertypeList" value="/content/documents/administration/website/parameter-type"/>
                <entry key="imagedistributiontagging" value="/content/documents/administration/website/imagedistributiontagging"/>
                <entry key="imagesourcepermission" value="/content/documents/administration/website/imagesourcepermission"/>
                <entry key="newstypes" value="/content/documents/administration/website/news-type"/>
                <entry key="frequency" value="/content/documents/administration/website/frequency"/>
                <entry key="datenamingconvention" value="/content/documents/administration/website/date-naming-convention"/>
            </map>
        </constructor-arg>
    </bean>
</beans>
